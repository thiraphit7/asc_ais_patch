{
  "name": "Huawei HG8145 Template",
  "description": "Full unlock for Huawei HG8145B7N and similar models",
  "version": "1.0",
  "sequences": [
    {
      "name": "disable_captcha",
      "description": "Disable captcha verification",
      "params": [
        {
          "Name": "InternetGatewayDevice.UserInterface.X_AIS_WebUserInfo.Captcha_enable",
          "Value": "0",
          "Type": "xsd:string"
        }
      ]
    },
    {
      "name": "unlock_superadmin",
      "description": "Disable SuperAdmin security lock",
      "params": [
        {
          "Name": "InternetGatewayDevice.UserInterface.X_AIS_WebUserInfo.SuperAdminSecurity",
          "Value": "0",
          "Type": "xsd:string"
        }
      ]
    },
    {
      "name": "enable_remote",
      "description": "Enable remote access",
      "params": [
        {
          "Name": "InternetGatewayDevice.UserInterface.X_AIS_WebUserInfo.RemoteAccess",
          "Value": "1",
          "Type": "xsd:string"
        }
      ]
    },
    {
      "name": "unlock_carrier",
      "description": "Disable carrier locking",
      "params": [
        {
          "Name": "InternetGatewayDevice.UserInterface.CarrierLocking.X_AIS_LockingEnable",
          "Value": "0",
          "Type": "xsd:string"
        }
      ]
    }
  ],
  "superadmin": {
    "enabled": false,
    "params": []
  },
  "wifi_config": {
    "enabled": false,
    "params": []
  }
}
